      <sch:pattern id="syriacaMainODD-lg-abstractModel-structure-l-constraint-11">
            <sch:rule context="tei:lg">
               <sch:report test="ancestor::tei:l[not(.//tei:note//tei:lg[. = current()])]">
        Abstract model violation: Lines may not contain line groups.
      </sch:report>

            </sch:rule>

         </sch:pattern>
      <ref name="tei_att.global.attributes"/>
      <ref name="tei_att.divLike.attributes"/>
      <ref name="tei_att.typed.attributes"/>
      <ref name="tei_att.declaring.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="tei_divGen">
