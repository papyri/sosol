      <zeroOrMore>
        <choice>
          <text/>
          <ref name="tei_model.gLike"/>
          <ref name="tei_model.phrase"/>
          <ref name="tei_model.inter"/>
          <ref name="tei_model.global"/>
        </choice>
      </zeroOrMore>
      <sch:pattern id="syriacaMainODD-l-abstractModel-structure-l-constraint-10">
            <sch:rule context="tei:l">
               <sch:report test="ancestor::tei:l[not(.//tei:note//tei:l[. = current()])]">
